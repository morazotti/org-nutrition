(defun org-nutrition-taco--to-plist (entry)
 "Convert a TACO list ENTRY to an org-nutrition catalog plist."
 (list :name (nth 0 entry)
 :calories (number-to-string (nth 1 entry))
 :protein (number-to-string (nth 2 entry))
 :carbs (number-to-string (nth 3 entry))
 :fats (number-to-string (nth 4 entry))
 :portion "100g"
 :type "taco"))

(defun org-nutrition-taco-entries ()
 "Return TACO entries as an alist of (NAME . PLIST), for use in completion."
 (mapcar (lambda (e)
 (cons (nth 0 e) (org-nutrition-taco--to-plist e)))
 org-nutrition-taco-entries))

